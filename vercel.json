{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "vercel-python@0.1.0"
        }
    }
}